{
  "name": "bugbirdco/yoke",
  "description": "A bolt-on laravel framework to ease the development of Atlassian Connect add-ons",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Jordan Bugbird",
      "email": "jordan@bugbird.co"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "bugbirdco/cabinet": "^1",
    "guzzlehttp/guzzle": "^7",
    "laravel/framework": ">=8",
    "firebase/php-jwt": "^5.4",
    "bugbirdco/in-time": "^1"
  },
  "autoload": {
    "psr-4": {
      "BugbirdCo\\Yoke\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BugbirdCo\\Yoke\\YokeServiceProvider"
      ],
      "aliases": {
        "Descriptor": "BugbirdCo\\Yoke\\Facades\\Descriptor"
      }
    }
  }
}
