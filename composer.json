{
    "name": "bugbirdco/yoke",
    "description": "A bolt-on laravel framework to ease the development of Atlassian Connect add-ons",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jordan Bugbird",
            "email": "jordan@bugbird.co"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "bugbirdco/cabinet": "^1.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "BugbirdCo\\Yoke\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BugbirdCo\\Yoke\\YokeServiceProvider"
            ],
            "aliases": {
                "Descriptor": "BugbirdCo\\Yoke\\DescriptorFacade"
            }
        }
    }
}
